movableFrom: aString	| maze field row string char block |	self flag: 'TODO' "Refactor?!! ... see #fixedFrom:".	maze := self mazeFrom: aString.	field := Field new.	1 to: maze size do: [:y |		string := maze at: y.		row := OrderedCollection new.		1 to: string size do: [:x |			char := string at: x.			block := self blockForMovable: char.			block isSokobanBlock ifTrue: [block location: x @ y].			row add: block].		field addRow: row].	^ field