keyStroke: anEvent 	self resetTypeAhead; deselect.	(self dispatchOnKeyboardEvent: anEvent) 		ifTrue:			[self doneTyping.			self storeSelectionInParagraph.			^self].	self openTypeIn.	self hasSelection ifTrue: [ "save highlighted characters"		UndoSelection := self selection].	self 		zapSelectionWith: self typeAhead contents ; 		resetTypeAhead ;		unselect ;		storeSelectionInParagraph