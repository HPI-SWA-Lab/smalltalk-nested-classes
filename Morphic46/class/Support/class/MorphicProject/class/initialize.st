initialize	"MorphicProject initialize"	"Initialize the default Morphic Project background"	self defaultFill: (SolidFillStyle color: Color white).	CurrentProject ifNil:		["This is the Top Project."		CurrentProject := super new initialProject.		CurrentProject spawnNewProcessAndTerminateOld: true].	self convertOldProjects.	Project current isMorphic ifTrue:[		"Set the default background in the current world"		Project current world color: self defaultFill.	].