squeakMenuOn: aDockingBar 	"Private - fill the given docking bar"		aDockingBar addItem: [ :item |		item			contents: '';			icon: MenuIcons squeakLogoIcon;			selectedIcon: MenuIcons squeakLogoInvertedIcon;			addSubMenu: [ :menu | 				self					aboutMenuItemOn: menu;					updateMenuItemOn: menu.				menu addLine.				self 					saveMenuItemOn: menu;					saveAsMenuItemOn: menu;					saveAsNewVersionMenuItemOn: menu.				menu addLine.				self					saveAndQuitMenuItemOn: menu;					quitMenuItemOn: menu ] ]