keyStroke: evt	| evtCharacter |	evtCharacter := evt keyCharacter.	evtCharacter = Character escape ifTrue: [		^cancelButton ifNotNil: [ cancelButton performAction ] ].	evtCharacter = Character cr ifTrue: [		^selectedButton ifNotNil: [ selectedButton performAction ] ].	(evtCharacter = Character arrowLeft or: [ 		evt shiftPressed and: [ evtCharacter = Character tab ] ]) ifTrue: [ 			^self selectPreviousButton ].	(evtCharacter = Character arrowRight or: [ 		evtCharacter = Character tab ]) ifTrue: [ 			^self selectNextButton ].