addPaneHSplitters	| remaining targetY sameY |	remaining := paneMorphs copy reject: [:each | each layoutFrame bottomFraction = 1].	[remaining notEmpty] whileTrue:		[targetY := remaining first layoutFrame bottomFraction.		sameY := paneMorphs select: [:each | each layoutFrame bottomFraction = targetY].		self addPaneHSplitterBetween: remaining first and: sameY.		remaining := remaining copyWithoutAll: sameY]